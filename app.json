{
  "expo": {
    "name": "AI Workout Builder",
    "slug": "ai-workout-builder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-xpv.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/app-icon-xpv.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.aiworkoutbuilder",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": []
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon-xpv.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.aiworkoutbuilder"
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/images/app-icon-xpv.png",
          "imageWidth": 200
        }
      ]
    ],
    "scheme": "aiworkoutbuilder",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "backendUrl": "https://3v7m36dq7b8b7nhzwcy3b6cud7ap7qwr.app.specular.dev"
    }
  }
}